{
  "name": "catsdb",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "10.15.3",
    "yarn": "1.13.0"
  },
  "scripts": {
    "clean:node_modules": "npx rimraf **/node_modules",
    "clean": "yarn clean:node_modules",
    "lint": "eslint .",
    "start": "NODE_ENV=development nodemon src/index.js --config 'config/nodemon.json'",
    "test:jest": "NODE_ENV=test jest --config config/jest/index.js",
    "test:watch": "yarn test:jest --watch",
    "test": "yarn lint && yarn test:jest"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "glob": "7.1.4",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "eslint": "5.15.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "jest": "24.8.0",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "supertest": "4.0.2"
  }
}
